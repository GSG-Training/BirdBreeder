<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.birdBreeder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BirdBreeder">
        <activity android:name="com.example.birdBreeder.View.ui.Activites.SignUpActivity" android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"/>
        <activity android:name="com.example.birdBreeder.View.ui.Activites.LogInActivity" android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name="com.example.birdBreeder.View.ui.Activites.MainActivity.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    <activity android:name="com.example.birdBreeder.View.ui.Activites.SplashActivity" android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"/>

    </application>

</manifest>